    "express": "^4.18.1",
    "mongoose": "^6.4.0",
    "morgan": "^1.10.0",
    "vue": "^3.2.13",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
